{
  "manifest_version": 3,
  "name": "ProfAI Side Panel",
  "version": "1.0",
  "description": "An AI assistant side panel for Chrome.",
  "permissions": ["sidePanel", "activeTab", "scripting"],
  "host_permissions": ["https://meet.google.com/*"],
  "background": { "service_worker": "background.js" },
  "side_panel": {
    "default_path": "panel.html"
  },
  "action": {
    "default_title": "Open ProfAI"
  },
  "icons": {
    "128": "public/vite.svg"
  }
}
